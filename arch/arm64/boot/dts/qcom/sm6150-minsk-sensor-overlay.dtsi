&tlmm {
	pen_int_default: pen_int_default {
			mux {
				pins = "gpio96";
				function =  "gpio";
			};
			config {
				pins = "gpio94";
				num-grp-pins = <1>;
				drive-strength = <2>;
				bias-no-pull;
			};
	};

	pen_int_sleep: pen_int_sleep {
			mux {
				pins = "gpio96";
				function =  "gpio";
			};
			config {
				pins = "gpio94";
				num-grp-pins = <1>;
				drive-strength = <2>;
				bias-no-pull;
			};
	};

	capsense_sar_gpio_pins: capsense_sar_gpio_pins {
		mux {
			pins = "gpio83";
			function = "gpio";
		};
		config {
			pins = "gpio83";
			drive-strength = <2>;
			bias-pull-up; /* PULL UP*/
			input-enable;
		};
	};
};

&soc {
	pen_detect{
		compatible = "rohm,bu520xx_pen_detect";
		pinctrl-names = "default","sleep";
		pinctrl-0 = <&pen_int_default>;
		pinctrl-1 = <&pen_int_sleep>;
		rohm,nirq-gpio=<&tlmm 96 0x02>;
	};
};

&soc{
	pm6150_l16{

		qcom,init-voltage = <2970000>;
		regulator-always-on;
		regulator-boot-on;
	};

};

&qupv3_se1_i2c {
	status = "ok";
	abov_sar@20 {
		status = "ok";
		compatible = "abov,abov_sar_mmi_overlay";
		reg = <0x20>;
		cap_vdd-supply = <&pm6150_l16>;
		interrupt-parent = <&tlmm>;
		interrupts = <83 0x2>;
		gpios = <&tlmm 83 0x2>;
		pinctrl-names = "default";
		pinctrl-0 = <&capsense_sar_gpio_pins>;
		reg_array_len = <0x03>;
		reg_array_val = <0x07 0x00 0x08 0x1F 0xFB 0x01>;
		cap,use_channel_ch1 = <0x00>;
		cap,use_channel_ch0 = <0x01>;
		cap,use_channel_ch2 = <0x02>;
		ch0_name ="capsense_bottom_right";
		ch1_name ="capsense_bottom_left";
		ch2_name ="capsense_top";
		label = "AbovCapSenseCA";
		cap,channel_number = <0x03>;
		};
};

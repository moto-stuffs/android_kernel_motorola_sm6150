CONFIG_TOUCHSCREEN_GOODIX_GTX8_MMI=y
CONFIG_TOUCHSCREEN_GOODIX_GTX8_MMI_UPDATE=y
CONFIG_TOUCHSCREEN_GOODIX_GTX8_MMI_GESTURE=y
CONFIG_TOUCHSCREEN_GOODIX_GTX8_MMI_TOOLS=y
